{
  "name": "ibizawebextension",
  "version": "2.0.0",
  "description": "Azure Portal Developer Web Extension",
  "private": true,
  "scripts": {
    "build": "SET NODE_OPTIONS=--openssl-legacy-provider && npx webpack --mode production",
    "build:dev": "SET NODE_OPTIONS=--openssl-legacy-provider && npx webpack --mode development",
    "watch": "SET NODE_OPTIONS=--openssl-legacy-provider && npx webpack --mode development --watch --progress",
    "lint": "npx eslint src/**/*.{ts,tsx,vue} --fix",
    "prettier": "npx prettier --write ./src/"
  },
  "repository": {
    "type": "git",
    "url": "https://git.pxlbuzzard.com/pxlbuzzard/IbizaWebExtension"
  },
  "keywords": [
    "web",
    "extension"
  ],
  "author": "Daniel Jost and David Wilson",
  "license": "MIT",
  "dependencies": {
    "element-plus": "1.0.2-beta.65",
    "vue": "^3.1.5"
  },
  "devDependencies": {
    "@types/chrome": "0.0.148",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "@vue/compiler-sfc": "^3.1.5",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^7.14.0",
    "html-webpack-plugin": "^5.3.2",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.4",
    "typescript": "^4.3.5",
    "vue-loader": "^16.3.1",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.47.0",
    "webpack-cli": "^4.7.2",
    "zip-webpack-plugin": "^4.0.1"
  }
}
